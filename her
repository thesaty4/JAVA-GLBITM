class father{
	String name,lastname;
	float height;
	void init(){
		
		name="hariram";
		lastname="mishra";
		height=5.5f;
	}
}

class child1 extends father{
	String name;
	void detail(){
		init();
		System.out.println("1st CHILD DETAILS \n");
		System.out.println("First Child : Satya");
		System.out.println("Last Name : "+lastname);
		System.out.println("Height : "+height);
	}
}

class child2 extends father{
	String name;
	void detail(){
		init();
		System.out.println("2nd CHILD DETAILS \n");
		System.out.println("First Child : Deep");
		System.out.println("Last Name : "+lastname);
		System.out.println("Height : "+height);
	}
}

class multilevelMain{
public static void main(String arg[]){
	child1 obj1 = new child1();
	child2 obj2 = new child2();
	obj1.detail();
	obj2.detail();
}
}
